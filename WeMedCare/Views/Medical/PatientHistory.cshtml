@using WeMedCare.Models

@{
    //Layout = null;
    int c = 1;
}
<link href="~/Content/DataTables/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet"/>
<style>
    .lll {
        width: 150px;
    }
    #tablepres {
        margin-top: 60px;
    }
    .hd {
        font-family: inherit;
        color: #007bff;
        margin-top: 25px;
        border-bottom: 2px solid currentColor;
        margin-bottom: 40px;
    }
</style>
<body>
<div class="container">
    <center>
        <div class="hd">
            <h1>History</h1>
        </div>
    </center>
    <div class="row">
        <div class="col-md-12">
            <table id="tablepres" class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>Serial No.</th>
                    <th>Date</th>
                    <th>Prescription</th>

                </tr>
                </thead>
                <tbody>
                @foreach (PatientAppointmentModel g in Model)
                {

                    <tr>
                        <td>@c</td>
                        <td>@g.Date</td>
                        <td>

                            <a href="#" onclick="window.open('../../@g.Prescription', '_blank');"><button type="button" class="btn btn-secondary lll">Prescription</button></a>

                        </td>
                    </tr>
                    c = c + 1;
                }
                </tbody>
            </table>
        </div>
    </div>
</div>
</body>
@section scripts
{

    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var table = $('#tablepres').DataTable();
        });
    </script>
}